{
    "name": "react-simple-demo",
    "version": "1.0.0",
    "description": "React simple demo",
    "main": "bin/server.js",
    "scripts": {
        "start": "concurrent --kill-others \"npm run start-prod\"",
        "start-prod": "better-npm-run start-prod",
        "build": "better-npm-run build",
        "postinstall": "npm run build",
        "start-dev": "better-npm-run start-dev",
        "dev": "concurrent --kill-others \"npm run start-dev\"",
        "test": "karma start",
        "test-node": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register",
        "test-node-watch": "./node_modules/mocha/bin/mocha $(find api -name '*-test.js') --compilers js:babel-core/register --watch"
    },
    "betterScripts": {
        "start-prod": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "production",
                "PORT": 8181
            }
        },
        "start-dev": {
            "command": "node ./bin/server.js",
            "env": {
                "NODE_PATH": "./src",
                "NODE_ENV": "development",
                "PORT": 4000
            }
        },
        "build": {
            "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nguyenduong127/react-simple.git"
    },
    "author": "Duong Nguyen <nguyenduong127@gmail.com>",
    "license": "ISC",
